    upAxis = "Y"
)

over "Chessboard"
{
    def Scope "Materials" (
        prepend references = @./Chessboard_mat.mtlx@</MaterialX/Materials>
    )
    {
    }
    over Mesh "Geom" (
        apiSchemas = ["MaterialBindingAPI"]
    )
    {
        rel material:binding = </Chessboard/Materials/M_Chessboard>
    }
}

